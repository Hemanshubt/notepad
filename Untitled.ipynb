   "source": [
    "def count_once(n, count=0):\n",
    "    while n > 0:\n",
    "        if n & 1 == 1:\n",
    "            count += 1\n",
    "        n >>= 1\n",
    "    return count\n",
    "\n",
    "a = int(input(\"Enter any number: \"))\n",
    "print(count_once(a))"
   ]
  }
